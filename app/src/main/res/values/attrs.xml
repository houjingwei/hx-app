<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LinearLayoutForListView">
        <attr name="mydivider" format="color"/>
        <attr name="mydividerHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MySeekBar">
        <attr name="textName" format="reference|string"/>
        <attr name="unit" format="reference|string"/>
        <attr name="textHight" format="reference|string"/>
        <attr name="processVal" format="reference|integer"/>
        <attr name="changeVal" format="reference|integer"/>
    </declare-styleable>

    <declare-styleable name="SignListLinearLayout">
        <attr name="txtTitle" format="reference|string"/>
        <attr name="valImage" format="reference|color"/>
    </declare-styleable>


    <declare-styleable name="ProgressWheel">
        <attr name="pw_text" format="string" />
        <attr name="pw_textColor" format="color" />
        <attr name="pw_textSize" format="dimension" />
        <attr name="pw_barColor" format="color" />
        <attr name="pw_rimColor" format="color" />
        <attr name="pw_rimWidth" format="dimension" />
        <attr name="pw_spinSpeed" format="dimension" />
        <attr name="pw_delayMillis" format="integer" />
        <attr name="pw_circleColor" format="color" />
        <attr name="pw_radius" format="dimension" />
        <attr name="pw_barWidth" format="dimension" />
        <attr name="pw_barLength" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomIndicator">
        <attr name="count_ci" format="integer" />
        <attr name="width_ci" format="dimension" />
        <attr name="height_ci" format="dimension" />
        <attr name="margin_ci" format="dimension" />
        <attr name="normal_icon" format="reference" />
        <attr name="selected_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="DropImageView">
        <attr name="textDraSize" format="dimension" />
        <attr name="textDraColor" format="color" />
        <attr name="textDra" format="string" />
        <attr name="is_zoom" format="boolean"/>
        <attr name="iconDra" format="reference" />
    </declare-styleable>
</resources>